<tr id="msg_#cid#">
    <td class="text-center" style="width: 70px;">
        <label class="css-input css-checkbox css-checkbox-rounded css-checkbox-primary">
            <input id="chk_#cid#" type="checkbox" value="#cid#"><span></span>
        </label>
    </td>
    <td class="font-w600 hidden-xs" style="width: 50px;" valign="top">
        <img src="#avatar#" class="img-avatar img-avatar48 #online_status#">
    </td>
    <td onclick="javascript:ViewMsg('#cid#');return false;">
        <a class="font-w600" href="#" onclick="javascript:ViewMsg('#cid#');return false;">#fullname#</a>
        <div class="text-muted push-5-t push-5">
            #read_status# <span class="font-w300">#msg#...</span>
        </div>
        <div class="text-muted h6 font-w300">
            <em>#date#</em>
        </div>
    </td>
</tr>